/* =Example Graph
-------------------------------------------------------------- */

.next-button {
        padding-top: 0px;
	padding-bottom: 0px;
	background: #f8f4f3;
}

.next-button .content {
	display: none;
	padding-bottom: 49px;
}

.next-button .graph.desktop {
	min-height: 100px;
	margin-bottom: -3.8%;
}

.next-button .graph.mobile {
	display: none;
}

.next-button .graph img {
	display: block;
	width: 100%;
	height: auto;
}

.next-button .graph .avocado {
	width: 105px;
	height: 114px;
	margin: 0 auto -72px;
	padding-left: 8px;
	background: url(images/example_graph_avocado.png) no-repeat right top;
	position: relative;
}

.next-button .text {
	width: 40%;
	position: relative;
	z-index: 20;
	margin: 0 2%;
	border: 1px solid;
	background: #f9f9f9;
	padding: 28px 3%;
}

.next-button .text h2 {
	position: absolute;
	top: -52px;
	left: 0;
	width: 100%;
	font-size: 60px;
	font-weight: 700;
	text-align: center;
	text-transform: uppercase;
}

.next-button .text h3 {
	margin-bottom: 9px;
	font-size: 16px;
	font-weight: 700;
	font-style: italic;
	text-align: center;
}

.next-button .text h3:before {
	content: open-quote;
}

.next-button .text h3:after {
	content: close-quote;
}

.next-button .text p {
	color: black;
	font-size: 14px;
	line-height: 17px;
}

.next-button .text .plant-state {
	display: table;
	width: 100%;
	margin: 24px 0 13px;
}

.next-button .text .plant-state ul {
	display: table-row;
}

.next-button .text .plant-state ul li {
	display: table-cell;
	width: 33%;
	text-align: center;
}

.next-button .text .plant-state ul li object {
	height: 46px;
	margin-bottom: 3px;
}

.next-button .text .plant-state li span {
	display: block;
	font-size: 14px;
	font-style: italic;
}

.next-button .text.before {
	float: left;
	margin-right: 0;
	border-color: #bcbfbc;
}

.next-button .text.before h2 {
	color: #777777;
}

.next-button .text.before h3 {
	color: #777777;
}

.next-button .text.before .plant-state li span {
	color: #777777;
}

.next-button .text.after {
	float: right;
	margin-left: 0;
	border-color: #aad29f;
}

.next-button .text.after h2 {
	color: #73841c;
}

.next-button .text.after h2:before {
	content: url(images/example_graph_leaf.png);
}

.next-button .text.after h3 {
	color: #73841c;
}

.next-button .text.after .plant-state li span {
	color: #73841c;
}

.next-button .text .arrow {
	display: none;
}

.next-button .toggle {
	height: 21px;
	margin-bottom: 21px;
	text-align: center;
	border-bottom: 1px solid #73841c;
}

.next-button .toggle .button {
	display: inline-block;
	padding: 0 9px;
        background: #f8f4f3;
}

.next-button .toggle .button a {
	display: inline-block;
	padding: 11px 16px 14px;
	color: #73841c;
	font-size: 16px;
	text-decoration: none;
	text-transform: uppercase;
	border: 1px solid #39b54a;
	border: 1px solid #73841c;
	position: relative;
}

.next-button .toggle .button a .open,
.next-button .toggle .button a .close {
	padding-right: 0px;
	/*background: url(images/see_example.png) no-repeat;*/
}

.next-button .toggle .button a .open {
	background-position: right top;
}

.next-button .toggle .button a .close {
	display: none;
	background-position: right bottom;
}

.next-button .toggle .button a.open .open {
	display: none;
}

.next-button .toggle .button a.open .close {
	display: inline;
}


@media all and (max-width: 767px) {
	.next-button .graph.desktop {
		margin-bottom: -1%;
	}
}

@media all and (max-width: 639px) {
	.next-button {
		padding-bottom: 34px;
	}

	.next-button .content {
		padding-bottom: 20px;
	}

	.next-button .graph.desktop {
		display: none;
	}

	.next-button .graph.mobile {
		display: block;
		margin: 5px -5%;
	}

	.next-button .text {
		float: none !important;
		width: auto;
		margin: 0;
		padding-top: 15px;
	}

	.next-button .text h2 {
		position: static;
		font-size: 18px;
		margin-bottom: 8px;
	}

	.next-button .text.after h2:before {
		content: '';
	}

	.next-button .text h3 {
		font-size: 12px;
	}

	.next-button .text p {
		height: auto !important;
		font-size: 12px;
		text-align: center;
	}

	.next-button .text .plant-state {
		margin: 12px 0 2px;
	}

	.next-button .text .plant-state li span {
		font-size: 12px;
	}

	.next-button .text .plant-state ul li object {
		width: 36px;
		height: 30px;
		margin-bottom: 6px;
	}

	.next-button .text .arrow {
		display: block;
		position: absolute;
		width: 20px;
		height: 12px;
		background: url(images/mobile/example_graph_arrow.png) no-repeat;
		background-size: 20px;
	}

	.next-button .text.before .arrow {
		left: 15px;
		bottom: -11px;
		background-position: bottom;
	}

	.next-button .text.after .arrow {
		right: 15px;
		top: -11px;
		background-position: top;
	}

	.next-button .toggle {
		border-bottom: none;
		margin-bottom: 22px;
	}

	.next-button .toggle .button {
		display: block;
		padding: 0 7%;
	}

	.next-button .toggle .button a {
		display: block;
		padding-right: 16px;
		background: none;
	}
}
