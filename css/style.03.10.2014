}


.gradient-grey, .gradient-gray {

    border: 1px solid;
    border-color: #73841D #63740D #4A5612;
    background: #C0C0C0;
    background: -moz-linear-gradient(top, #C0C0C0, #898989);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #C0C0C0), color-stop(100%, #898989));
    background: -webkit-linear-gradient(top, #C0C0C0, #898989);
    background: -o-linear-gradient(top, #C0C0C0, #898989);
    background: -ms-linear-gradient(top, #C0C0C0, #898989);
    background: linear-gradient(top, #C0C0C0, #898989);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#C0C0C0', endColorstr='#898989',GradientType=0);
    border-radius: 3px;
    -moz-border-radius: 3px;
    -webkit-box-shadow: 0 1px 0 rgba(255, 255, 255, 0.6) inset;
    -moz-box-shadow: 0 1px 0 rgba(255, 255, 255, 0.6) inset;
    box-shadow: 0 1px 0 rgba(255, 255, 255, 0.6) inset;
}

